<h2>Do you forgot your password</h1>
    {{#if error}}
    {{error}}
    {{/if}}
    <form action="/auth/reset" method="post" class="p-3">
        <div class="row">
            <div class="input-field col s12">
                <input placeholder="email" value="lol@kek.com" id="first_name" type="email" name="email"
                    class="validate">
                <label for="first_name">email</label>
            </div>
            <input type="hidden" name="_csrf" value="{{csurf}}">
            <button class="btn waves-effect waves-light" type="submit">
                Submit
            </button>
        </div>
    </form>