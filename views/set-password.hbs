<h2>Set Password</h1>
    {{#if error}}
    {{error}}
    {{/if}}
    <form action="/auth/reset/password" method="post" class="p-3">
        <div class="row">
            <div class="input-field col s12">
                <input placeholder="password" value="lol@kek.com" id="first_name" type="password" name="password"
                    class="validate">
                <label for="first_name">password</label>
            </div>
            <input type="hidden" name="_csrf" value="{{csurf}}">
            <input type="hidden" name="token" value="{{token}}">
            <button class="btn waves-effect waves-light" type="submit">
                change password
            </button>
        </div>
    </form>